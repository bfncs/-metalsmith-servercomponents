{
	"extends" : [
		"airbnb"
	],
	"rules": {
		"no-tabs": 0,
		"no-console": 0,
		"global-require": 0,
		"indent" : [2, "tab", { "SwitchCase": 1 }],
		"max-len": [2, 140, 2],
		"import/no-unresolved": 0,
		"prefer-template": 0,
		"new-cap": 0,
		"arrow-body-style": 0,
		"no-unused-expressions": 0
	}
}
